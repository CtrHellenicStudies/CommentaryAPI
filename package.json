    "pre-commit": "^1.2.2",
    "react-autocomplete": "^1.5.10",
    "react-s3-uploader": "^4.2.0",
    "request": "^2.81.0",
    "request-promise": "^4.2.2",
    "slugify": "^1.2.6",
    "subscriptions-transport-ws": "^0.8.1",
    "supertest": "^3.1.0",
    "twitter": "^1.7.1",
    "underscore": "^1.8.3",
    "underscore.string": "^3.3.4",
    "uuid": "^3.1.0",
    "winston": "^3.0.0",
    "zxcvbn": "^4.4.2"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.3",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-stage-1": "^6.24.1",
    "babel-tape-runner": "^2.0.1",
    "concurrently": "^3.4.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-plugin-flowtype": "^2.35.0",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.0.1",
    "faker": "^4.1.0",
    "faucet": "^0.0.1",
    "flow-bin": "^0.51.0",
    "jest": "^23.4.2",
    "mockingoose": "^2.9.1",
    "nodemon": "^1.11.0",
    "regenerator-runtime": "^0.10.5",
    "shortid": "^2.2.8"
  },
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:staged": "lint-staged",
    "test": "jest src/__tests__/db.test.js && jest --verbose --coverage --detectOpenHandles",
    "server": "NODE_ENV=development nodemon ./src/index.js --exec babel-node",
    "start": "npm run server",
    "db:seed": "babel-node scripts/run-db-seed",
    "db:clear": "babel-node scripts/run-db-clear",
    "db:add-passage-index": "babel-node scripts/add-passage-index",
    "db:setup": "npm run db:clear && npm run db:seed",
    "build": "babel src -d build",
    "execute": "yarn pm2 start build/index.js --name ahcip-api-express",
    "build-and-execute": "concurrently \"yarn execute\" \"yarn build\"",
    "jsdoc": "jsdoc src -r -c .jsdoc.json -d docs"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/CtrHellenicStudies/CommentaryAPI.git"
  },
  "pre-commit": "lint:staged",
  "lint-staged": {
    "*.{md,sh,txt,xml,html,json}": [
      "editorconfig-tools fix",
      "git add"
    ],
    "*.{js,jsx}": [
      "eslint --fix",
      "git add"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!<rootDir>/node_modules/",
      "!<rootDir>/docs/",
      "!<rootDir>/public/",
      "!<rootDir>/build/",
      "!<rootDir>/coverage/",
      "!<rootDir>/k8s/"
    ]
  },
  "author": "The Center for Hellenic Studies",
  "license": "MIT"
}
