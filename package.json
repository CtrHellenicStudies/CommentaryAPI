    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "dataloader": "^1.3.0",
    "dotenv": "^4.0.0",
    "editorconfig-tools": "^0.1.1",
    "email-templates": "^2.7.1",
    "eventemitter3": "^2.0.3",
    "express": "4.15.3",
    "express-session": "^1.15.3",
    "graphql": "^0.10.3",
    "graphql-date": "^1.0.3",
    "graphql-errors": "^2.1.0",
    "graphql-iso-date": "^3.3.0",
