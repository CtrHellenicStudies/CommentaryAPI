    "lint-staged": "^6.0.0",
    "mdi": "^2.0.46",
    "migrate": "^1.1.1",
    "migrate-mongoose": "^3.2.2",
    "mongoose": "^4.13.9",
    "mongoose-paginate": "^5.0.3",
    "mongoose-schema-to-graphql": "^2.5.7",
    "mongoose-timestamp": "^0.6.0",
    "mongoose-type-email": "^1.0.5",
    "mongoose-type-url": "^1.0.2",
    "mongoose-url-slugs": "^1.0.0",
    "morgan": "^1.8.2",
    "nodemailer": "^4.1.0",
    "oauth-shim": "^1.1.1",
